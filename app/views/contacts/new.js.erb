var errors = JSON.parse("<%= j @contact.errors.to_json.html_safe %>");
var response = 'Ocurrió un error al envíar tu contacto:<br>' + (function() {
	for (var key in errors) return errors[key][0];
})();
Utils.insertFormResponse(document.getElementById('new_contact'), response, 2000);
Utils.notification('notice', 'Revisá los datos.');
Utils.loader().hide();
console.info('Contact Error', errors);